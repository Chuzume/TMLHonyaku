	}

	"Nudge Minion UI Right": {
		DisplayName: ミニオンUIを右に動かす
	}

	"Nudge Minion UI Left": {
		DisplayName: ミニオンUIを左に動かす
	}

	"Nudge Minion UI Down": {
		DisplayName: ミニオンUIを下に動かす
	}

	"Nudge Minion UI Up": {
		DisplayName: ミニオンUIを上に動かす
	}

	"Toggle Summoner UI Layer": {
		DisplayName: サモナーUIのレイヤー切り替え
	}

	"Toggle Summoner UI Visibilty": {
		DisplayName: サモナーUIの表示/非表示を切り替える
	}
